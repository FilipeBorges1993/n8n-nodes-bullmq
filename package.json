{
  "name": "n8n-nodes-bullmq",
  "version": "0.1.1",
  "description": "N8n nodes for BullMQ",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://www.npmjs.com/package/n8n-nodes-bullmq",
  "author": {
    "name": "@minhlucvan",
    "email": "luk.mink@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minhlucvan/n8n-nodes-bullmq.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "gulp clean && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "nodes/Bullmq/Bullmq.node.js",
      "nodes/Bullmq/BullmqTrigger.node.js",
      "nodes/Bullmq/BullmqRespomd.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "bullmq": "^5.12.14",
    "install": "^0.13.0",
    "ioredis": "^5.4.1"
  }
}
